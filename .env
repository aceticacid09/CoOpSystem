# Database (Railway will auto-provide if using Railway PostgreSQL)
POSTGRES_HOST=<railway-postgres-host>
POSTGRES_USER=<railway-postgres-user>
POSTGRES_PASSWORD=<railway-postgres-password>
POSTGRES_DB=<your-database-name>
POSTGRES_PORT=5432
POSTGRES_SSLMODE=require

# Application
APP_PORT=8000

# Security
JWT_SECRET=<generate-a-strong-secret-key>

# CORS - Add your frontend domains
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:8000,https://co-op-system.vercel.app/

VITE_API_BASE_URL=coopsystem-production.up.railway.app